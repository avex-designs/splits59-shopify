<div class="cs-pages  static-page">

    {% comment %}
        All cs (customer service) pages in a nav.
    {% endcomment %}
    <div class="cs-pages__nav js-header-offset">

        {% for link in linklists["cs-pages"].links %}

            {% assign linked_page_obj = link.object %}

            <a href="{{ linked_page_obj.url }}" class="cs-pages__nav-item  {% if link.active %}cs-pages__nav-item--active{% endif %}">{{ link.title }}</a>

        {% endfor %}

    </div>

    {% comment %}
        Display current page content.
    {% endcomment %}
    <div class="cs-pages__content">
        <h1 class="cs-pages__page-title  static-page__title">{{ page.title }}</h1>
        <div class="rte">{{ page.content }}</div>
    </div>

</div>
